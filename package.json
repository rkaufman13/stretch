{
  "name": "stretch-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-react": "^4.3.3",
    "firebase": "^10.14.1",
    "firebaseui": "^6.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-button": "^0.8.0",
    "react-router-dom": "^6.28.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "serve": "vite",
